27d \u12e8\u1218\u1228\u1218\u1229 \u12e8\u1292\u120e\u1272\u12ad \u1363 \u12e8\u12a9\u123d\u1272\u12ad \u12a5\u1293 \u12e8\u1263\u1295\u1271 \u12e8\u12d8\u122d \u1210\u1228\u130e\u127d \u12a8\u120c\u120e\u1279 \u130b\u122d \u12e8\u1270\u1208\u12eb\u12e8 \u12f2\u130d\u122a\u12ce\u127d \u1290\u1260\u122f\u1278\u12cd \u1361\u1361 \u1260\u1270\u1328\u121b\u122a\u121d \u1260\u12a0\u1320\u1243\u120b\u12ed \u12e8\u1304\u1294\u1272\u12ad \u1235\u1265\u1235\u1266\u127d \u12a8\u124b\u1295\u124b \u121d\u12f0\u1263 \u130b\u122d \u12e8\u121a\u1323\u1323\u1219 \u1218\u1206\u1293\u1278\u12cd\u1295 \u12e8\u12ac\u1295\u12eb\u12cd \u1209\u12a6\u1295 \u1328\u121d\u122e \u12a8\u121a\u1273\u12c8\u1241 \u1260\u1235\u1270\u1240\u122d \u1361\u1361 \u12e8\u1292\u120e-\u1233\u1205\u122b\u1295 \u1270\u1293\u130b\u122a\u12ce\u127d \u1262\u1206\u1291\u121d \u1363 \u12e8\u1209\u12ce \u12ad\u120b\u1235\u1270\u122d \u12a8\u1292\u1300\u122d-\u12ae\u122d\u12f6\u134b\u1292\u12eb\u1295 \u1270\u1293\u130b\u122a \u1215\u12dd\u1266\u127d \u130b\u122d \u1260\u12d9\u122a\u12eb\u1278\u12cd \u12ed\u1308\u129b\u120d \u1361\u1361 \u1260\u12f0\u1261\u1263\u12ca\u12cd \u1209\u12a6 \u134d\u120d\u1230\u1275 \u12c8\u1245\u1275 \u12ed\u1205 \u12a8\u134d\u1270\u129b \u12e8\u1206\u1290 \u12f5\u1265\u120d\u1245\u1290\u1275\u1295 \u12e8\u121a\u12eb\u1218\u1208\u12ad\u1275 \u1218\u1206\u1291\u1295 \u12eb\u1218\u120b\u12ad\u1273\u1209 \u1361\u1361 \u12e8\u12ab\u120c\u1295\u1302\u1295 \u1261\u12f5\u1296\u127d \u12a5\u1293 \u12e8\u121b\u1233\u12ed \u1261\u12f5\u1296\u127d \u12a0\u1290\u1235\u1270\u129b \u12e8\u1263\u1295\u1271 \u12dd\u122d\u12eb \u12eb\u120b\u1278\u12cd \u130d\u1295 \u1309\u120d\u1205 \u12e8\u12a9\u123d \u12dd\u122d\u12eb \u12eb\u120b\u1278\u12cd \u1206\u1290\u12cd \u1270\u1308\u129d\u1270\u12cb\u120d \u1361\u1361 [4]\n\u1260\u1325\u1295\u1273\u12ca \u12f2 \u12a4\u1295 \u12a4 \u1260\u1325\u1293\u1275 \u12e8\u1273\u12c8\u1240 \u12e8\u1304\u1294\u1272\u12ad \u121d\u1201\u122d \u12f4\u126a\u12f5 \u122a\u12ed\