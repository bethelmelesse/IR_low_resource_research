5 \u12ed\u127b\u120b\u120d\u1362\n\u1373\u136d. \u1325\u1242\u1275 \u12e8\u12a6\u122e\u121e \u121d\u1201\u122b\u1295 \u1260\u1235\u1215\u1270\u1275 \u12a6\u122e\u121d\u129b 10 \u12a5\u1295\u12da\u122b\u1295 \u1232\u12eb\u1235\u1348\u120d\u1308\u12cd \u130d\u12d5\u12dd \u1230\u1263\u1275 \u1265\u127b \u12a3\u1208\u12cd \u12ed\u120b\u1209\u1362 \u120b\u1272\u1295 \u12a3\u121d\u1235\u1275 \u12cb\u12e8\u120e\u127d \u12a3\u1209\u1275 \u1270\u1265\u120e \u12a3\u121d\u1235\u1271\u1291 \u12cb\u12e8\u120e\u127d \u12a5\u12e8\u12f0\u122b\u1228\u1261 \u12a6\u122e\u121d\u129b \u12a3\u1225\u122d \u12cb\u12e8\u120e\u127d \u12a3\u1209\u1275 \u121b\u1208\u1275 \n\u12a6\u122e\u121d\u129b\u12cd\u1295 \u1218\u1233\u12f0\u1265 \u1290\u12cd\u1362 \u12cb\u12e8\u120d \u1218\u12f0\u122b\u1228\u1265\u121d \u1275\u122d\u1349 \u1235\u134d\u122b \u121b\u1263\u12a8\u1295 \u1290\u12cd\u1362 \u12e8\u120b\u1272\u1295 \u12cb\u12e8\u120e\u127d \u12a3\u121d\u1235\u1275 \u1293\u1278\u12cd \u1262\u1263\u120d \u12a5\u1295\u12b3\u1295 \u1208\u12a6\u122e\u121d\u129b \u12a3\u121d\u1235\u1275 \u12cb\u12e8\u120e\u127d \u1260\u1242 \u12a3\u12ed\u12f0\u1209\u121d\u1362 \u12e8\u1241\u1264 \u12cb\u12e8\u120e\u127d \u12a3\u121d\u1235\u1275 \u12a8\u1206\u1291 \u12a8\u12a3\u121d\u1235\u1275 \u1260\u120b\u12ed \u12a5\u1295\u12da\u122b\u1295 \u1208\u12a3\u120b\u1278\u12cd \u12e8\u130d\u12d5\u12dd \u1240\u1208\u121e\u127d \u1208\u1218\u1320\u1240\u121d \u12a8\u120b\u1272\u1291 \u1241\u1264 \u12e8\u1260\u12d9 \u12e8\u12a5\u1235\u1354\u120a\u1295\u130d \u127d\u130d\u122e\u127d \u12ed\u1348\u1325\u122b\u1209\u1362 \u1208\u121d\u1233\u120c \u12eb\u1205\u120d \u201c\u12f8\u201d\u1295 \u1260\u1235\u121d\u1295\u1275 \u12cb\u12e8\u120e\u1279\u00a0\u1208\u1218\u12ad\u1270\u1265 \u1208\u1241\u1264 \u120b\u1272\u1295 \u12a820 \u1260\u120b\u12ed \u1240\u1208\u121e\u127d \u1232\u12eb\u1235\u1348\u120d\u1309\u1275 \u1208\u130d\u12d5\u12d9 \u136f \u1260\u1242 \u1293\u1278\u12cd\u1362 \n\u1373\u136e. \u1325\u1242\u1275 \u12e8\u12a6\u1