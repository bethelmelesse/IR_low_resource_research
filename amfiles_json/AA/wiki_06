b\u1296\u1273\u12ca \u1290\u12cd \u12e8\u121a\u1209\u121d \u12a3\u1209\u1362 \"\u12d3\u12ed\u1295\"\u1293 \"\u12d3\u1218\u1275\" \u12e8\u1320\u1260\u1241 \u1293\u1278\u12cd \u1262\u1263\u120d \u12e8\u12c8\u1295\u12d9\u1295 \u1235\u121d \u1260\"\u12a3\u1263\u12ed\" \u12e8\u121a\u133d\u1349 \u12a3\u1209\u1362 \u1235\u1208\u12da\u1205 \u12a5\u1290\u12da\u1205 \u1290\u1308\u122e\u127d \u1325\u1293\u1275 \u12eb\u1235\u1348\u120d\u130b\u1278\u12cb\u120d\u1362 \u1208\u121d\u1233\u120c \u12eb\u1205\u120d \"\u12a3\u1263\u12ed\" \u12a5\u1293 \"\u12d3\u1263\u12ed\"\u1295 \u1260\u12f5\u121d\u133d \u1218\u1208\u12e8\u1275 \u12ed\u127b\u120b\u120d \u12a5\u1295\u12f3\u12ed\u1263\u120d \u12e8\u121a\u1320\u1265\u1240\u12cd\u1293 \u12e8\u121a\u120b\u120b\u12cd \"\u1263\" \u1240\u1208\u121d \u1235\u1208\u1206\u1290 \u1290\u12cd\u1362 \u1374\u1370. \u12e8\u121e\u12ad\u123c \u1270\u1265\u12ec\u12ce\u1279 \u1218\u1296\u122d \u12d3\u121b\u122d\u129b\u12cd\u1295 \u12a3\u12f3\u1260\u1228\u12cd \u12a5\u1295\u1302 \u12a3\u120d\u130e\u12f3\u12cd\u121d\u1362 \u12ed\u1205 \u134a\u12f0\u1209\u1295 \u12e8\u121a\u1320\u1240\u1219 \u120c\u120e\u127d \u124b\u1295\u124b\u12ce\u127d\u1295\u121d \u120a\u1218\u1208\u12a8\u1275 \u12ed\u127d\u120b\u120d\u1362 \u12e8\u120b\u1272\u1295 \u134a\u12f0\u120d \u1235\u1208\u12a3\u1235\u1278\u1308\u1228 \u1348\u1228\u1295\u1306\u127d \u12a3\u12f3\u12f2\u1235 \u134a\u12f0\u120b\u1275 \u12a5\u12e8\u1348\u1320\u1229 \u12a5\u12eb\u123b\u123b\u1209\u1275 \u1290\u12cd\u1362 \u1374\u1371. \u12e8\u121e\u12ad\u123c \u134a\u12f0\u120d \u1309\u12f3\u12ed \u130a\u12dc\u12cd \u12e8\u12a3\u1208\u1348\u1260\u1275 \u12ad\u122d\u12ad\u122d \u1290\u12cd\u1362 \u1260\u12ae\u121d\u1355\u12e9\u1270\u122d \u12d8\u1218\u1295 \u1274\u12ad\u1296\u120e\u1305\u12cd \u127d\u130d\u122e\u127b\u127d\u1295\u1295 \u1235\u1208\u121a\u1348\u1273 \u134a\u12f0\u120d \u1208\u1218\u1240\u1290\u1235 \u1218\u12a8\u122b\u12a8\u122d \u130d\u12d5\u12db\u12ca \u12a3\u12ed\u12f0\u1208\u121d\u1362 \u1275\u12ad\